{
    "CERT_FILE": "ssl/cert.pem",
    "KEY_FILE": "ssl/key.pem",
    "FAVICON_FILE": "favicon.ico",
    "INDEX_FILE": "templates/index.html",
    "LOGIN_FILE": "templates/login.html",
    "CHAT_FILE":"templates/chat.html",
    "FORBIDDEN_FILE": "templates/403.html",
    "NOT_FOUND_FILE": "templates/404.html",
    "LOG_FILE": "SilkRoad.log",
    "LOGIN_PATH": "/login",
    "FAVICON_PATH": "/favicon.ico",
    "ENABLE_KEEP_ALIVE": true,
    "KEEP_ALIVE_TIMEOUT": 60,
    "KEEP_ALIVE_MAX": 100,
    "LOGIN_VERIFICATION_ENABLED": false,
    "RANDOM_UA_ENABLED": false,
    "CACHE_ENABLED": false,
    "CACHE_HTML": false,
    "CACHE_MEDIA": false,
    "CACHE_OTHER": false,
    "CACHE_LARGE_FILES": false,
    "BLACKLIST_ENABLED": true,
    "SCRIPT_INJECTION_ENABLED": true,
    "CLEAR_CLIENT_CACHE_ENABLED": true,
    "SERVER_NAME": "SilkRoad/3.1",
    "SESSION_COOKIE_NAME": "SilkRoad_session",
    "SCHEME": "https",
    "DOMAIN": "127.0.0.1",
    "BIND_IP": "",
    "PORT":8080,
    "SERVER": "https://127.0.0.1:8080/",
    
    "TEMPLATE_ENCODING": "utf-8",
    
    "PERFORMANCE": {
        "HTTP_CLIENT_POOL_SIZE": 100,
        "MAX_WORKER_THREADS": 200,
        "SESSION_CACHE_TTL": 300,
        "LOG_SAMPLE_RATE": 0.1,
        "ENABLE_PERFORMANCE_MODE": true
    },
    
    "HTTP": {
        "ENABLE_HTTP2": true,
        "MAX_RETRIES": 3,
        "TIMEOUT": 30.0
    },
    
    "STREAM": {
        "BUFFER_SIZE": 8192,
        "ASYNC_PRELOAD": true,
        "ASYNC_PRELOAD_MAX_SIZE": 50,
        "LARGE_FILE_THRESHOLD": 1048576
    },
    
    "CACHE_CONTROL": {
        "BY_MIME": {
            "text/html": 3600,
            "text/css": 86400,
            "application/javascript": 86400,
            "image/*": 604800,
            "video/*": 604800,
            "audio/*": 604800,
            "application/octet-stream": 86400,
            "*": 3600
        },
        "MAX_AGE": 86400,
        "MAX_SIZE": 524288000
    }
}
